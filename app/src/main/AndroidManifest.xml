<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ubuntu.newnew">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".InputActivity" />
        <activity android:name=".EditRemoveActivity" />
        <activity android:name=".TypeSelectionActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".PieActivity" />
        <activity android:name=".BarActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".Credits"></activity>
    </application>

</manifest>